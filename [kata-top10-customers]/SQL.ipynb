   "source": [
    "SELECT c.customer_id, \n",
    "email, \n",
    "COUNT(pa.payment_id) as payments_count,\n",
    "SUM(pa.amount)::FLOAT as total_amount\n",
    "\n",
    "FROM customer as c\n",
    "INNER JOIN payment as pa\n",
    "ON c.customer_id = pa.customer_id\n",
    "\n",
    "group by c.customer_id\n",
    "order by total_amount DESC\n",
    "LIMIT 10"
   ]
  }
 ],
